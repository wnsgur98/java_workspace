package teamtest;

import java.util.Scanner;

public class TeamTask02 {
   public static void main(String[] args) {
      
//      10ì§„ìˆ˜ â†’ 2ì§„ìˆ˜ ë³€í™˜í•˜ê¸°(4bit)
      
//      0~15ë²”ìœ„ ë°–ì˜ ìˆ«ìë¥¼ ì…ë ¥ë°›ì„ ì‹œ â€œì˜ëª»ëœ ì…ë ¥ì…ë‹ˆë‹¤ ë‹¤ì‹œ ì…ë ¥í•´ì£¼ì„¸ìš” : â€œë¥¼ ì¶œë ¥í•˜ê³  ì œëŒ€ë¡œ ë°›ì„ ë•Œê¹Œì§€ ë°˜ë³µ
//
//      0ì€ 0ìœ¼ë¡œ ë§Œë“  5*5í¬ê¸°ì˜ 0ìœ¼ë¡œ ë§Œë“¤ ê²ƒ
//
//      1ì€ 1ìœ¼ë¡œ ë§Œë“  5*5í¬ê¸°ì˜ 1ë¡œ ë§Œë“¤ ê²ƒ (1ì€ ì¤‘ê°„ì— ìˆì–´ì•¼í•¨ ì™¼ìª½ ë˜ëŠ” ì˜¤ë¥¸ìª½ìœ¼ë¡œ ì¹˜ìš°ì¹˜ë©´ ì•ˆë¨)
//
//      10ì§„ìˆ˜ë¥¼ 2ì§„ìˆ˜ë¡œ ë³€í™˜í•˜ëŠ” â€œtoBinaryString()â€ ì‚¬ìš©ê¸ˆì§€
//
//      0ë˜ëŠ” 1ì„ ì¶œë ¥í•œ ë’¤ ì¤„ë°”ê¿ˆì„ í•  ê²ƒ
//
//      ë©”ì†Œë“œë¡œ ë§Œë“¤ì–´ì„œ ì‚¬ìš©í•  ê²ƒ : 2ì§„ìˆ˜ë¡œ ë³€í™˜í•˜ëŠ” ì½”ë“œ, 0ì„ ì¶œë ¥í•˜ëŠ” ì½”ë“œ, 1ì„ ì¶œë ¥í•˜ëŠ” ì½”ë“œ (ì´ 3ê°€ì§€)
//
//      (ì „ë¶€ ë§¤ê°œë³€ìˆ˜ ììœ , ë¦¬í„´ê°’ ììœ , static ì‚¬ìš© ë¶ˆê°€)
      

      
      // 0~15ê¹Œì§€ì˜ ìˆ«ìë¥¼ ì…ë ¥ë°›ì€ ë’¤ ì´ë¥¼ 4ë¹„íŠ¸ì˜ ì´ì§„ìˆ˜ë¡œ ì¶œë ¥
      TeamTask02 tt = new TeamTask02();
      Scanner sc = new Scanner(System.in);
      
      int num = 0;
      int[] binaryArray = new int[4];

      // ì…ë ¥ ë©”ì‹œì§€ ì¶œë ¥
      System.out.print("0ë¶€í„° 15ê¹Œì§€ì˜ ìˆ«ìë¥¼ ì…ë ¥í•´ì£¼ì„¸ìš” : ");
      num = sc.nextInt();
      sc.nextLine(); // void buffer

      while (num > 15 || num < 0) {
         System.out.print("ì˜ëª»ëœ ì…ë ¥ì…ë‹ˆë‹¤ ë‹¤ì‹œ ì…ë ¥í•´ì£¼ì„¸ìš” : ");
         num = sc.nextInt();
         sc.nextLine(); // void buffer
      }
      
      //10to2
      tt.decimalToBinary(binaryArray, num);

      // ì¶œë ¥
      for (int data : binaryArray) {
         if (data == 0) {
            tt.print0();
            System.out.println();
         } else {
            tt.print1();
            System.out.println();
         }
      }
      
      sc.close();

   }
   
   // ğŸŒšğŸŒšğŸŒšğŸŒšğŸŒš
   // ğŸŒš     ğŸŒš
   // ğŸŒš     ğŸŒš
   // ğŸŒš     ğŸŒš
   // ğŸŒšğŸŒšğŸŒšğŸŒšğŸŒš
   //print 0
   //ì§€ë‚œë²ˆì— í–ˆë˜ ì†ì´ ë¹ˆ ì‚¬ê°í˜• ì°¸ê³ 
   //ê°™ì€ í˜•ì‹ìœ¼ë¡œ 0,4ë²ˆì§¸ í–‰ë ¬ ì±„ì›€
   void print0() {
      for (int i = 0; i < 5; i++) {
         for (int j = 0; j < 5; j++) {
            if (i == 0 || i == 4 || j == 0 || j == 4) {
               System.out.print("0");
            } else {
               System.out.print(" ");
            }
         }
         System.out.println();
      }
   }
   
   //print 1
   //ì¤‘ê°„ì¸ 2ì—´ë§Œ 1ë¡œ ì±„ì›€
   void print1() {
      for (int i = 0; i < 5; i++) {
         for (int j = 0; j < 5; j++) {
            if (j == 2) {
               System.out.print("1");
            } else {
               System.out.print(" ");
            }
         }
         System.out.println();
      }
   }

   void decimalToBinary(int[] array, int num) {
      // ì´ì§„ìˆ˜ë¡œ ë³€í™˜ 8421
      // ì´ì§„ìˆ˜ë¡œ ë³€í™˜ì‹œí‚¤ëŠ” ë°©ë²• ì¤‘ 10ì§„ìˆ˜ë¥¼ 2ë¡œ ë‚˜ëˆ„ì—ˆì„ ë•Œ ë‚˜ë¨¸ì§€ë“¤ë¡œ
      // ê³„ì‚°í•˜ëŠ” ë°©ë²• ì‚¬ìš©í•¨
      // ë’¤ì˜ ë°°ì—´ë¶€í„° ì±„ìš´ ì´ìœ ëŠ” ê³„ì‚° ë°©ë²• ë•Œë¬¸ì„ (10to2 ì•„ë˜ ì°¸ê³ )
      for (int i = array.length - 1; i >= 0; i--) {
         if (num / 2 >= 1) {
            array[i] = num % 2;
            num /= 2;
         } else if (num == 1) {
            array[i] = 1;
            break;
         } else {
            break;
         }
      }
   }
}
